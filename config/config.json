{
  "development": {
   "use_env_variable": "DATABASE_URL",
   "dialect": "postgres",
   "logging": false
  },
  "production": {
   "use_env_variable": "DATABASE_URL",
   "dialect": "postgres",
   "logging": false,
   "dialectOptions": {
     "ssl": {
       "require": true,
       "rejectUnauthorized": false
     }
   },
   "pool": {
     "max": 5,
     "min": 0,
     "acquire": 30000,
     "idle": 10000
   }
}
}
